    "scripts": {
        "build": "rm -rf dist/ && tsc",
        "start": "npm run build && node dist/server.js",
        "watch": "nodemon --watch './**/*.{ts}' src/server.ts",
        "dev": "tsc-watch --onSuccess \"npm run watch\""
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "chalk": "4.1.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "joi": "^17.6.0",
        "mongoose": "^6.3.5",
        "nanoid": "3.3.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/nanoid": "^3.0.0",
        "@types/node": "^17.0.40",
        "nodemon": "^2.0.16",
        "ts-node": "^10.8.1",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.7.3"
    }
}
